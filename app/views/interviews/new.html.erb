<br>
<div class=flex-container4>
<div class="text-center">
  <h3>Lets create a new company!</h3>
</div>
<div class="form-container">
  <%= simple_form_for [@user, @interview] do |f| %>
  <div class="flex-container2">
    <div class="form-style-5">
      <%= f.input :company, placeholder: "Name e.g. Amazon"%>
      <%= f.input :industry, placeholder: "Tech"%>
      <%= f.input :photo, as: :file %>
    </div>
  </div>
  <div>
    <%= f.submit class: "btn btn-primary", value: "Create Company" %>
    </div>
  <% end %>
  </div>
</div>
