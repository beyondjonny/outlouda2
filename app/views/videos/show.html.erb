<div class="text-center">
  <h3>Candidate - <%= @video.name %></h3>
</div>
<div class="flex-container">
  <%= render 'reviews/new' %>
  <% @video.reviews.each do |review| %>
  <div class="card">
    <div class="card-body">
      <%= review.rating %>
      <%= review.comment %>
    </div>
  </div>
  <% end %>
</div>
